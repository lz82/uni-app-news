.nav-bar-wrapper .nav-bar-fixed {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  background: #f07373;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-flow: column nowrap;
          flex-flow: column nowrap;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding: 0 15rpx;
  box-sizing: border-box;
}
.nav-bar-wrapper .nav-bar-fixed .nav-bar-content {
  height: 45px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.nav-bar-wrapper .nav-bar-fixed .nav-bar-content .nav-bar-search {
  background: #fff;
  color: #999;
  width: 100%;
  height: 30px;
  border-radius: 15px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15rpx;
  box-sizing: border-box;
}
.nav-bar-wrapper .nav-bar-fixed .nav-bar-content .nav-bar-search .icon-search {
  width: 20px;
  height: 20px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 20px 20px;
  margin-right: 15rpx;
}
.nav-bar-wrapper .nav-bar-fixed .nav-bar-content .nav-bar-search .search-txt {
  font-size: 14px;
}
.nav-bar-wrapper .nav-bar-placeholder {
  height: 65px;
}

